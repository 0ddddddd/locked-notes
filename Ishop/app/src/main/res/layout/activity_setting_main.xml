<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#fff"
    android:orientation="vertical"
    android:gravity="center_horizontal">
    <LinearLayout
        android:layout_width="345dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <com.localworld.ishop.common.image.ISImageView
            android:id="@+id/iv_setting_headpic"
            android:layout_width="66dp"
            android:layout_height="66dp"
            android:background="@drawable/ic_setting_main"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="10dp"
            app:roundAsCircle="true"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/layout_setting_collection"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <TextView
                        android:id="@+id/tv_setting_collectionnum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="111"
                        android:textStyle="bold"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                    <TextView
                        android:id="@+id/tv_setting_collection"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="收藏"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layout_setting_track"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <TextView
                        android:id="@+id/tv_setting_tracknum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="1211"
                        android:textStyle="bold"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                    <TextView
                        android:id="@+id/tv_setting_track"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="足迹"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layout_setting_follow"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <TextView
                        android:id="@+id/tv_setting_follownum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:text="11"
                        android:textStyle="bold"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                    <TextView
                        android:id="@+id/tv_setting_follow"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_gravity="center"
                        android:text="关注"
                        android:textSize="13sp"
                        android:textColor="#000"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_setting_blank"
                    android:layout_width="58dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:textSize="13sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="5dp">
                <Button
                    android:id="@+id/btn_setting_personinfo"
                    android:layout_width="221dp"
                    android:layout_height="23dp"
                    android:background="@drawable/payment_button_shape"
                    android:text="个人信息"
                    android:textSize="13sp"/>
                <ImageView
                    android:id="@+id/iv_setting_setting"
                    android:layout_width="27dp"
                    android:layout_height="23dp"
                    android:background="@drawable/payment_button_shape"
                    android:layout_marginLeft="5dp"
                    android:src="@drawable/ipole_icon_settings_gray"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <ScrollView
        android:id="@+id/scroll_setting_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp">
            <LinearLayout
                android:id="@+id/layout_setting_main_motto"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:layout_marginBottom="10dp"
                android:layout_marginTop="10dp"
                android:layout_gravity="center_horizontal">
                <TextView
                    android:id="@+id/tv_setting_username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:text="Ludwig"
                    android:textSize="12sp"
                    android:textColor="#000"
                    android:textStyle="bold"/>
                <TextView
                    android:id="@+id/tv_setting_usermotto"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="lalalalllll"
                    android:textColor="#000"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_setting_main_order"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:background="@drawable/payment_button_shape">
                <TextView
                    android:layout_width="345dp"
                    android:layout_height="28dp"
                    android:layout_marginLeft="20dp"
                    android:text="订单"
                    android:textSize="14sp"
                    android:textColor="#000"
                    android:gravity="center_vertical"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorLightGray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp">
                    <LinearLayout
                        android:id="@+id/layout_setting_unpay"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="27dp"
                            android:layout_height="23dp"
                            android:src="@drawable/ipole_icon_wait_payment"
                            android:layout_gravity="center"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="待付款"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:textColor="#000"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_setting_paid"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="25dp"
                            android:layout_height="21dp"
                            android:src="@drawable/ipole_icon_finish_payment"
                            android:layout_gravity="center"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="已付款"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:textColor="#000"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_setting_main_transit"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:layout_gravity="center_horizontal"
                android:orientation="vertical"
                android:background="@drawable/payment_button_shape"
                android:gravity="center">
                <TextView
                    android:layout_width="345dp"
                    android:layout_height="28dp"
                    android:layout_marginLeft="20dp"
                    android:text="物流"
                    android:gravity="center_vertical"
                    android:textSize="14sp"
                    android:textColor="#000"/>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorLightGray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="10dp">
                    <LinearLayout
                        android:id="@+id/layout_setting_transit_undeliver"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="30dp"
                            android:layout_height="23dp"
                            android:src="@drawable/ipole_icon_7"
                            android:layout_gravity="center"
                            android:layout_marginBottom="5dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="待发货"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:textColor="#000"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_setting_transit_delivered"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="36dp"
                            android:layout_height="24dp"
                            android:src="@drawable/ipole_icon_6"
                            android:layout_gravity="center"
                            android:layout_marginBottom="5dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="已发货"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:textColor="#000"/>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/layout_setting_transit_get"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:orientation="vertical">
                        <ImageView
                            android:layout_width="29dp"
                            android:layout_height="28dp"
                            android:src="@drawable/ipole_icon_transit_dark"
                            android:layout_gravity="center"
                            android:layout_marginBottom="5dp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="已签收"
                            android:textSize="10sp"
                            android:layout_gravity="center"
                            android:textColor="#000"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/layout_setting_transitinfor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:background="@color/colorLightGray"
                        android:layout_marginLeft="40dp"
                        android:layout_marginRight="40dp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="center"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp">
                        <com.localworld.ishop.common.image.ISImageView
                            android:id="@+id/iv_setting_transit_goodpic"
                            android:layout_width="43dp"
                            android:layout_height="43dp"
                            app:roundedCornerRadius="3dp"
                            android:layout_marginRight="5dp"/>
                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp">
                            <LinearLayout
                                android:layout_width="220dp"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">
                                <TextView
                                    android:id="@+id/tv_setting_startplace"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:text="北京"
                                    android:textSize="12sp"
                                    android:textColor="#000"/>
                                <LinearLayout
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:orientation="horizontal"
                                    android:gravity="center">
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="预计还需"
                                        android:textSize="9sp"
                                        android:textColor="#000"/>
                                    <TextView
                                        android:id="@+id/tv_setting_transittime"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="24"
                                        android:textSize="9sp"
                                        android:textColor="#000"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:gravity="center"
                                        android:text="小时"
                                        android:textSize="9sp"
                                        android:textColor="#000"/>
                                </LinearLayout>
                                <TextView
                                    android:id="@+id/tv_setting_endplace"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:gravity="end"
                                    android:text="杭州"
                                    android:textSize="12sp"
                                    android:textColor="#000"/>
                            </LinearLayout>
                            <ImageView
                                android:layout_width="209dp"
                                android:layout_height="12dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="2dp"
                                android:src="@drawable/ic_setting_delivered_all"/>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorLightGray"/>
                <ImageView
                    android:id="@+id/iv_setting_transit"
                    android:layout_width="21dp"
                    android:layout_height="8dp"
                    android:src="@drawable/ipole_icon_16"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"/>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_setting_main_discount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="vertical"
                android:background="@drawable/payment_button_shape"
                android:paddingBottom="10dp"
                android:layout_gravity="center_horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="28dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:text="优惠"
                        android:textSize="14sp"
                        android:textColor="#000"/>
                    <TextView
                        android:id="@+id/tv_setting_open"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="展开"
                        android:textSize="13sp"
                        android:layout_marginRight="20dp"
                        android:textColor="@color/colorPrimaryDark"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorLightGray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center_vertical"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="P点:"
                        android:textSize="12sp"
                        android:textColor="#000"
                        android:layout_marginLeft="20dp"/>
                    <TextView
                        android:id="@+id/tv_setting_points"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="200"
                        android:textSize="12sp"
                        android:textColor="#000"
                        android:textStyle="bold"
                        android:layout_marginLeft="10dp"/>
                    <ImageView
                        android:id="@+id/iv_setting_discount_imagep"
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:src="@drawable/ipole_icon_33"
                        android:layout_marginRight="20dp"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="优惠券"
                    android:textSize="12sp"
                    android:textColor="#000"
                    android:layout_marginBottom="20dp"
                    android:layout_marginLeft="20dp"/>
                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recycler_setting_discount"
                    android:layout_width="308dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:visibility="gone">
                </android.support.v7.widget.RecyclerView>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/layout_setting_main_friends"
                android:layout_width="match_parent"
                android:layout_height="153dp"
                android:layout_marginBottom="10dp"
                android:orientation="vertical"
                android:background="@drawable/payment_button_shape"
                android:layout_gravity="center_horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="28dp"
                    android:orientation="horizontal"
                    android:gravity="center_vertical">
                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="2dp"
                        android:layout_marginLeft="20dp"
                        android:text="朋友"
                        android:textSize="14sp"
                        android:textColor="#000"/>
                    <TextView
                        android:id="@+id/tv_setting_openall"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:layout_marginBottom="2dp"
                        android:background="#fff"
                        android:text="展开全部"
                        android:layout_marginRight="20dp"
                        android:textSize="13sp"
                        android:textColor="@color/colorPrimaryDark"/>
                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/colorLightGray"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_marginTop="10dp">
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/payment_button_shape"
                        android:layout_marginRight="2dp">
                        <com.localworld.ishop.common.image.ISImageView
                            android:id="@+id/iv_setting_friendspic1"
                            app:roundAsCircle="true"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:background="@drawable/ic_setting_main"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <TextView
                            android:id="@+id/tv_setting_friendsname1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Beethoven"
                            android:textAllCaps="false"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            android:textColor="#000"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <Button
                            android:id="@+id/btn_setting_call1"
                            android:layout_width="60dp"
                            android:layout_height="23dp"
                            android:text="Call"
                            android:textAllCaps="false"
                            android:textSize="12sp"
                            android:background="@drawable/payment_button_shape_4"
                            android:textColor="#fff"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/payment_button_shape"
                        android:layout_marginRight="2dp">
                        <com.localworld.ishop.common.image.ISImageView
                            android:id="@+id/iv_setting_friendspic2"
                            app:roundAsCircle="true"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:background="@drawable/ic_setting_main"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <TextView
                            android:id="@+id/tv_setting_friendsname2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Beethoven"
                            android:textAllCaps="false"
                            android:textSize="10sp"
                            android:textStyle="bold"
                            android:textColor="#000"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <Button
                            android:id="@+id/btn_setting_call2"
                            android:layout_width="60dp"
                            android:layout_height="23dp"
                            android:text="Call"
                            android:textAllCaps="false"
                            android:textSize="12sp"
                            android:background="@drawable/payment_button_shape_4"
                            android:textColor="#fff"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/payment_button_shape"
                        android:layout_marginRight="2dp">
                        <com.localworld.ishop.common.image.ISImageView
                            android:id="@+id/iv_setting_friendspic3"
                            app:roundAsCircle="true"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:background="@drawable/ic_setting_main"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <TextView
                            android:id="@+id/tv_setting_friendsname3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Beethoven"
                            android:textAllCaps="false"
                            android:textStyle="bold"
                            android:textSize="10sp"
                            android:textColor="#000"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <Button
                            android:id="@+id/btn_setting_call3"
                            android:layout_width="60dp"
                            android:layout_height="23dp"
                            android:text="Call"
                            android:textAllCaps="false"
                            android:textSize="12sp"
                            android:background="@drawable/payment_button_shape_4"
                            android:textColor="#fff"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="75dp"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:background="@drawable/payment_button_shape"
                        android:layout_marginRight="2dp">
                        <com.localworld.ishop.common.image.ISImageView
                            android:id="@+id/iv_setting_friendspic4"
                            app:roundAsCircle="true"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:background="@drawable/ic_setting_main"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <TextView
                            android:id="@+id/tv_setting_friendsname4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Beethoven"
                            android:textAllCaps="false"
                            android:textStyle="bold"
                            android:textSize="10sp"
                            android:textColor="#000"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"/>
                        <Button
                            android:id="@+id/btn_setting_call4"
                            android:layout_width="60dp"
                            android:layout_height="23dp"
                            android:text="Call"
                            android:textAllCaps="false"
                            android:textSize="12sp"
                            android:background="@drawable/payment_button_shape_4"
                            android:textColor="#fff"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="5dp"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
