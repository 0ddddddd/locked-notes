<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qiangqiang.ldnotebookapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".LdLoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LDNoteDetailActivity" />

        <provider
            android:name=".LDNoteProvider"
            android:authorities="com.example.qiangqiang.ldnotebookapplication.provider"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".MainActivity" />
    </application>

</manifest>